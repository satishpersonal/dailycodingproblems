public class MultiplyArray {
    public static List<Integer> multiplyArray(List<Integer> numList){
        List<Integer> resultList = new List<Integer>();
        Integer arraySize = numList.size();
        For(Integer i=0;i<arraySize;i++){
            Integer result = 1;
            For(Integer j=0;j<arraySize;j++){
                If(j!=i){
                    result = result*numList[j];
                }
            }
            resultList.add(result);
        }
        return resultList;
    }
}